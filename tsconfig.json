{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "allowJs": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitReturns": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "noImplicitAny": true,
    "incremental": true,
    "paths": {
      "~/*": [
        "./*"
      ]
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/layout.ts.old"],
  "exclude": [
    "node_modules",
    "./src/pages/index.tsx",
    "./src/pages/_app.tsx",
    "./tailwind.config.js",
    "**/*.jsx",
    "src/components/Avatar/Avatar.tsx",
    "src/pages/_middleware.ts",
  ]
}

