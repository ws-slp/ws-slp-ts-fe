{
  "name": "ws-slp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-types": "tsc",
    "dev": "next dev -p 3005",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "gts lint",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "format-check": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel type-check format-check lint build",
    "clean": "gts clean",
    "compile": "tsc --skipLibCheck && gts clean",
    "fix": "gts fix",
    "prepare": "husky install",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fontsource/work-sans": "^4.5.1",
    "@reach/alert": "^0.17.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@supabase/supabase-js": "^1.11.8",
    "@tsndr/cloudflare-worker-jwt": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "next": "^12.3.0",
    "next-seo": "^5.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.3.0",
    "daisyui": "^2.28.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gts": "^3.1.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": "16.15.0",
    "npm": "8.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 npm-run-all --parallel type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "npm run lint"
    ],
    "*.+(js|json|ts|tsx)": [
      "npm run format"
    ]
  }
}
