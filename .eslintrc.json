{
  "root": true,
  "extends": "./node_modules/gts/",
  "rules": {
    "node/no-unpublished-import": [
      "error",
      {
        "allowModules": ["protractor"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module",
    "warnOnUnsupportedTypeScriptVersion": false
  }
}
